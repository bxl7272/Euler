public class Euler5{
  public static void main (String[] args){
    int result = 0;
    for(int i = 19; i+=19){
      if (i%17 == 0){
        if (i%13 == 0){
          if (i%11 == 0){
            if (i%7 == 0){
              if (i%5 == 0){
                if (i%3 == 0){
                  if (i%2 == 0){
                    System.out.println(i);
                    break;
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
